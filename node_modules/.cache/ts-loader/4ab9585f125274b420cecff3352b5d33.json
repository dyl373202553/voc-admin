CH;AACJ;AAED,OAAM,SAAU,GAAV,CAAc,KAAd,EAA+C;AAAA,MAArB,MAAqB,uEAAZ,YAAY;AACjD,SAAO,KAAK,CAAC,KAAD,CAAL,CAAa,MAAb,CAAoB,MAApB,CAAP;AACH;AAED,OAAM,SAAU,WAAV,CAAsB,KAAtB,EAAmC;AACrC,MAAI,OAAO,CAAC,GAAR,CAAY,QAAZ,KAAyB,aAA7B,EAA4C;AACxC,oCAAyB,KAAzB;AACH,GAFD,MAEO;AACH,qDAA0C,KAA1C;AACH;AACJ","sourcesContent":["import dayjs from \"dayjs\"\r\nexport function deepCopy(templateData: object, type = -1, field = \"\") {\r\n    let data = JSON.parse(JSON.stringify(templateData))\r\n    switch (type) {\r\n        case 0:\r\n            data.shift()\r\n            break;\r\n        case 1:\r\n            data.pop()\r\n            break;\r\n        case 2:\r\n            data.pop()\r\n            data.shift()\r\n            break;\r\n        case 3:\r\n            data.splice(data.indexOf(field), 1)\r\n            break;\r\n    }\r\n    return data;\r\n}\r\n\r\nexport function getSlotData(value: Array<any>, type = -1, ...field: Array<string>) {\r\n    let data = value.reduce((pre: Array<string>, item: any) => {\r\n        if (item.slot) pre.push(item.slot);\r\n        return pre;\r\n    }, []);\r\n\r\n\r\n    if (type != -1) {\r\n        for (let i in field) {\r\n            data = deepCopy(data, type, field[i])\r\n        }\r\n    }\r\n\r\n    return data;\r\n}\r\n\r\nexport function getImg(value: string) {\r\n    if (process.env.NODE_ENV === 'development') {\r\n        return `/abc/resources/bluepage/a/${value}_A.jpg`;\r\n    } else {\r\n        return `http://211.136.25.148:6080/resources/bluepage/a/${value}_A.jpg`;\r\n    }\r\n}\r\n\r\nexport function day(value: any, format = \"YYYY-MM-DD\") {\r\n    return dayjs(value).format(format);\r\n}\r\n\r\nexport function getFileIcon(value: string) {\r\n    if (process.env.NODE_ENV === 'development') {\r\n        return `/abc/resources/${value}`;\r\n    } else {\r\n        return `http://172.30.200.237/resources/${value}`;\r\n    }\r\n}"],"sourceRoot":""}]}